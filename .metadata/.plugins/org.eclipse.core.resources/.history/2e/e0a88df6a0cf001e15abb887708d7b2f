package org.kh.java;

public class Variable4 {

	public static void main(String[] args) {
		String name = "\'박은성\'";
		short age = 25;
		float height = 180.8f;
		double weight = 97.7f;
		char init = "k";
		System.out.println("이름" +name);
		/* 이스케이프 문자 
		  \n : 줄바꿈
		  \t : 탭
		  \\ : \를 출력
		 */

	}

}
