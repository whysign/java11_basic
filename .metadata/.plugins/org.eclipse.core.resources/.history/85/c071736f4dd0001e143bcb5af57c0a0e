package org.kh.java;
// 논리 연산자 : &&[and],|[or],![느낌표는 부정의 의미 ]
public class Operation6 {

	public static void main(String[] args) {
	 int a = 26;
	 int b = 28;
	 int c = 27;
	 
	 System.out.println("a>b && a > c" +(a > b && b > c));
	 System.out.println("a>b && b > c" +(a > b && b > c));
	 System.out.println("a>b && b > c" +(a < b && b > c));
		
		
		

	}

}
